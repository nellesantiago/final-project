<%= form_with model: like, url: like_index_path do |form| %>

  <div class="field">
    <%= form.hidden_field :user_id, value: user_id %>
  </div>

  <div class="field">
    <%= form.hidden_field :post_id, value: post_id %>
  </div>

  <span class="actions">
    <%= button_tag :submit, class: "like-post-button" do %>
    <%= image_tag "unlike.png"%>
    <% end %>
  </span>
<% end %>